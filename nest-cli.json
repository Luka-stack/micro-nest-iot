{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/machines/src",
  "monorepo": true,
  "root": "apps/machines",
  "compilerOptions": {
    "webpack": false,
    "builder": "webpack",
    "tsConfigPath": "apps/machines/tsconfig.app.json"
  },
  "projects": {
    "kepware": {
      "type": "application",
      "root": "apps/kepware",
      "entryFile": "main",
      "sourceRoot": "apps/kepware/src",
      "compilerOptions": {
        "tsConfigPath": "apps/kepware/tsconfig.app.json"
      }
    },
    "communication": {
      "type": "library",
      "root": "libs/communication",
      "entryFile": "index",
      "sourceRoot": "libs/communication/src",
      "compilerOptions": {
        "tsConfigPath": "libs/communication/tsconfig.lib.json"
      }
    },
    "database": {
      "type": "library",
      "root": "libs/database",
      "entryFile": "index",
      "sourceRoot": "libs/database/src",
      "compilerOptions": {
        "tsConfigPath": "libs/database/tsconfig.lib.json"
      }
    },
    "machines": {
      "type": "application",
      "root": "apps/machines",
      "entryFile": "main",
      "sourceRoot": "apps/machines/src",
      "compilerOptions": {
        "tsConfigPath": "apps/machines/tsconfig.app.json"
      }
    },
    "analyser": {
      "type": "application",
      "root": "apps/analyser",
      "entryFile": "main",
      "sourceRoot": "apps/analyser/src",
      "compilerOptions": {
        "tsConfigPath": "apps/analyser/tsconfig.app.json"
      }
    }
  }
}
